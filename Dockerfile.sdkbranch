FROM supervisely/base-py-sdk:6.73.274

WORKDIR /app

COPY dev_requirements.txt /app/

# Uncomment the line below after releasing the new SDK version
# RUN pip install --no-cache-dir -r dev_requirements.txt

# development requirements
ARG SDK_VERSION
RUN pip install --no-cache-dir git+https://github.com/supervisely/supervisely.git@test-web-py-with-solutions
RUN git clone -b test-web-py-with-solutions https://github.com/supervisely/supervisely.git /app/supervisely

CMD ["bash"]
